{
  "name": "@augejs/provider-scanner",
  "version": "1.0.3",
  "description": "`provider-scanner` is a scan framework for nodejs.",
  "main": "dist/main.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "build": "rimraf dist && tsc --build ./tsconfig.json",
    "deploy": "npm run build && npm publish --access public",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build:apiDoc": "rimraf apiDocs && typedoc --tsconfig ./tsconfig.json --options ./typedoc.config.json && mkdir -p ./apiDocs/docs/assets && cp -r ./docs/assets/* ./apiDocs/docs/assets",
    "deploy:apiDoc": "npm run build:apiDoc && gh-pages -d apiDocs"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augejs/provider-scanner.git"
  },
  "author": "Alex Zhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/augejs/provider-scanner/issues"
  },
  "homepage": "https://github.com/augejs/provider-scanner#readme",
  "dependencies": {
    "extend": "^3.0.2",
    "@types/extend": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "gh-pages": "^3.1.0",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.33",
    "typescript": "^4.2.3"
  }
}
